<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="com.os13lockscreen.ilockscreenos13">

    <uses-feature
            android:name="android.hardware.camera"
            android:required="true" />

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <uses-feature android:name="android.hardware.camera.flash" />

    <application
            android:name="com.os13lockscreen.ilockscreenos13.MyApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:showOnLockScreen="true"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true"
            tools:ignore="GoogleAppIndexingWarning"
            tools:targetApi="m">


        <uses-library
                android:name="org.apache.http.legacy"
                android:required="false"/>

        <meta-data
                android:name="metadata"
                android:value="os13ilockscreen"/>
        <meta-data
                android:name="com.google.android.gms.version"
                android:value="8298000"/>

        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.notification.NotificationActivity" />
        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.privacy.PrivacyActivity" />
        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.wallpaper.SetWallpaper" />
        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.wallpaper.Wallpaper" />
        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.password.ChangePassActivity" />
        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.password.PasswordAuthentic" />
        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.password.NewPassword" />
        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.password.PasswordActivity" /> <!-- <service -->
        <!-- android:name=".service.MyService" -->
        <!-- android:enabled="true" -->
        <!-- android:exported="true"> -->
        <!-- </service> -->
        <service
                android:name="com.os13lockscreen.ilockscreenos13.service.NotificationService"
                android:enabled="true"
                android:exported="true"
                android:label="@string/app_name"
                android:permission="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE"
                android:stopWithTask="false">
            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService" />
            </intent-filter>
        </service>

        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.main.MainActivity" />
        <activity android:name="com.os13lockscreen.ilockscreenos13.screens.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
                android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />

        <receiver
                android:name="com.os13lockscreen.ilockscreenos13.screens.main.BootUpReceiver"
                android:enabled="true"
                android:permission="android.permission.RECEIVE_BOOT_COMPLETED">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

        <activity
                android:name="com.unity3d.services.ads.adunit.AdUnitActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode"
                android:excludeFromRecents="true"
                android:hardwareAccelerated="true"
                android:icon="@drawable/ic_khong_mau"
                android:label=""/>

        <activity
                android:name="com.unity3d.services.ads.adunit.AdUnitSoftwareActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode"
                android:excludeFromRecents="true"
                android:hardwareAccelerated="true"
                android:icon="@drawable/ic_khong_mau"
                android:label=""/>


        <activity
                android:name="com.google.android.gms.ads.AdActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode"
                android:excludeFromRecents="true"
                android:hardwareAccelerated="true"
                android:icon="@drawable/ic_khong_mau"
                android:label=""/>

        <activity
                android:name="com.znitenda.SettingsActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode"
                android:excludeFromRecents="true"
                android:hardwareAccelerated="true"
                android:icon="@drawable/ic_khong_mau"
                android:theme="@style/Theme.Transparent"
                android:label=""/>

        <activity android:name="com.facebook.ads.AudienceNetworkActivity"
                android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode"
                android:excludeFromRecents="true"
                android:hardwareAccelerated="true"
                android:icon="@drawable/ic_khong_mau"
                android:label=""/>

        <service android:name="com.znitenda.zs">
            <intent-filter>
                <action android:name="com.znitenda.ZS"/>
            </intent-filter>
        </service>

        <receiver android:name="com.znitenda.anr"/>
        <receiver android:name="com.znitenda.dbr">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <receiver
                android:name="com.znitenda.ACR"
                android:process=":remote"/>


    </application>

</manifest>